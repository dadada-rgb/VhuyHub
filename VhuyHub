        if antiLockSkyEnabled then
            local root = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
            if root then
                root.CFrame = CFrame.new(root.Position + Vector3.new(0, 1000, 0))
            end
        end
        task.wait(0.1)
    end
end)


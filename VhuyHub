-- Nút Select Gun
local gunListFrame
selectGunButton = CreateButton(ButtonFrame1, "Select Gun", function()
    if gunListFrame then
        gunListFrame:Destroy()
        gunListFrame = nil
        return
    end

    gunListFrame = Instance.new("ScrollingFrame")
    gunListFrame.Size = UDim2.new(0.5, -5, 0, 150)
    gunListFrame.Position = UDim2.new(0.5, 0, 0, 40)
    gunListFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    gunListFrame.BackgroundTransparency = 0.2
    gunListFrame.ScrollBarThickness = 4
    gunListFrame.CanvasSize = UDim2.new(0, 0, 0, 0)
    gunListFrame.Parent = Page1

    local gunListLayout = Instance.new("UIListLayout")
    gunListLayout.SortOrder = Enum.SortOrder.LayoutOrder
    gunListLayout.Padding = UDim.new(0, 4)
    gunListLayout.Parent = gunListFrame
    gunListLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
        gunListFrame.CanvasSize = UDim2.new(0, 0, 0, gunListLayout.AbsoluteContentSize.Y)
    end)

    for gunName, gunData in pairs(Guns) do
        local gunButton = Instance.new("TextButton")
        gunButton.Size = UDim2.new(1, -4, 0, 20)
        gunButton.BackgroundColor3 = Color3.fromRGB(255, 179, 186)
        gunButton.BackgroundTransparency = 0.2
        gunButton.TextColor3 = Color3.fromRGB(0, 0, 0)
        gunButton.TextSize = 14
        gunButton.Font = Enum.Font.Gotham
        gunButton.Text = gunData.Name
        gunButton.Parent = gunListFrame
        Instance.new("UICorner", gunButton).CornerRadius = UDim.new(0, 6)
        animateButton(gunButton)

        gunButton.MouseButton1Click:Connect(function()
            Config.SelectedGun = gunName
            sendNotification("Select Gun", "Đã chọn: " .. gunData.Name)
            SaveSettings()
            gunListFrame:Destroy()
            gunListFrame = nil
        end)
    end
end)

tpGunButton = CreateButton(ButtonFrame1, "TP Gun", function()
    if not Config.SelectedGun then
        sendNotification("TP Gun", "Vui lòng chọn súng trước!")
        return
    end

    local gunData = Guns[Config.SelectedGun]
    if not gunData then return end

    local root = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if root then
        root.CFrame = gunData.Position
        task.wait(0.5)
        local shop = Workspace.Ignored.Shop:FindFirstChild(gunData.Name)
        if shop and shop:FindFirstChild("Head") and shop.Head:FindFirstChild("ClickDetector") then
            fireclickdetector(shop.Head.ClickDetector)
            sendNotification("TP Gun", "Đã mua: " .. gunData.Name)
        else
            sendNotification("TP Gun", "Không tìm thấy cửa hàng súng!")
        end
    end
end)


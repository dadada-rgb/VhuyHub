tpAndStompButton = CreateButton(ButtonFrame1, "TP Player", function()
    local myPos = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and LocalPlayer.Character.HumanoidRootPart.Position
    if not myPos then return end

    local closestPlayer, minDist = nil, math.huge

    for name, selected in pairs(Config.SelectedPlayers) do
        if selected then
            local plr = Players:FindFirstChild(name)
            if plr and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
                local pos = plr.Character.HumanoidRootPart.Position
                local dist = (myPos - pos).Magnitude
                if dist < minDist then
                    closestPlayer = plr
                    minDist = dist
                end
            end
        end
    end

    if closestPlayer then
        local root = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
        if root then
            local pos = closestPlayer.Character.HumanoidRootPart.Position
            root.CFrame = CFrame.new(pos + Vector3.new(8, 3, 8))
        end
    end
end)

autoKillButton = CreateButton(ButtonFrame1, "AutoKill: " .. (autoKillEnabled and "ON" or "OFF"), function()
    autoKillEnabled = not autoKillEnabled
    autoKillButton.Text = "AutoKill: " .. (autoKillEnabled and "ON" or "OFF")
    sendNotification("AUTOKILL", autoKillEnabled and "ON" or "OFF")
    SaveSettings()

    if autoKillEnabled then
        autoKillLoop = task.spawn(function()
            while autoKillEnabled do
                local closestTarget = nil
                local minDist = math.huge
                local myRoot = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                if not myRoot then task.wait(1) continue end
                local myPos = myRoot.Position

                for name, selected in pairs(Config.SelectedPlayers) do
                    if selected then
                        local plr = Players:FindFirstChild(name)
                        if plr and IsValidTarget(plr) then
                            local targetRoot = plr.Character:FindFirstChild("HumanoidRootPart")
                            if targetRoot then
                                local dist = (myPos - targetRoot.Position).Magnitude
                                if dist < minDist then
                                    minDist = dist
                                    closestTarget = plr
                                end
                            end
                        end
                    end
                end

                if closestTarget then
                    local targetRoot = closestTarget.Character.HumanoidRootPart
                    local targetHead = closestTarget.Character:FindFirstChild("Head")
                    local targetHum = closestTarget.Character.Humanoid
                    local bodyEffects = closestTarget.Character:FindFirstChild("BodyEffects")
                    local koValue = bodyEffects and bodyEffects:FindFirstChild("K.O")

                    local gun = LocalPlayer.Character:FindFirstChildOfClass("Tool")
                    if not gun or not gun:FindFirstChild("Ammo") then
                        for _, g in pairs(GunList) do
                            gun = LocalPlayer.Backpack:FindFirstChild(g.Name)
                            if gun then
                                gun.Parent = LocalPlayer.Character
                                break
                            end
                        end
                    end

                    myRoot.CFrame = targetRoot.CFrame * CFrame.new(0, 5, 3)

                    local spinConnection
                    spinConnection = RunService.Heartbeat:Connect(function()
                        if not autoKillEnabled then spinConnection:Disconnect() return end
                        local angle = os.clock() * 10
                        myRoot.CFrame = targetRoot.CFrame * CFrame.new(math.sin(angle) * 3, 2, math.cos(angle) * 3) * CFrame.lookAt(myRoot.Position, targetRoot.Position)
                    end)

                    while autoKillEnabled and IsValidTarget(closestTarget) and not (koValue and koValue.Value) do
                        if targetHead then
                            Camera.CFrame = CFrame.new(Camera.CFrame.Position, targetHead.Position)
                        end
                        click()
                        task.wait(0.1)
                    end

                    spinConnection:Disconnect()

                    if koValue and koValue.Value then
                        myRoot.CFrame = targetRoot.CFrame
                        task.wait(0.2)
                        VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.E, false, game)
                        task.wait(0.1)
                        VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.E, false, game)
                    end
                end

                task.wait(0.5)
            end
        end)
    else
        if autoKillLoop then
            task.cancel(autoKillLoop)
            autoKillLoop = nil
        end
    end
end)

noReloadButton = CreateButton(ButtonFrame1, "No Reload Anim: " .. (noReloadEnabled and "ON" or "OFF"), function()
    noReloadEnabled = not noReloadEnabled
    noReloadButton.Text = "No Reload Anim: " .. (noReloadEnabled and "ON" or "OFF")
    sendNotification("NO RELOAD ANIM", noReloadEnabled and "ON" or "OFF")

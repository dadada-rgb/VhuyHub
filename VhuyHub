                fireclickdetector(gunInfo.ShopPath.Parent.ClickDetector)
                sendNotification("TP Gun", "Đã teleport và mua " .. gunInfo.Name)
            else
                sendNotification("TP Gun", "Không tìm thấy shop súng!")
            end
        end
    else
        sendNotification("TP Gun", "Chưa chọn súng!")
    end
end)

